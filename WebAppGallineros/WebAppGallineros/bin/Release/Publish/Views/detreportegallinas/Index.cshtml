@model IEnumerable<WebAppGallineros.Entities.detreportegallinas>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutIn.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.gallinas.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.reporte.Folio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.statusgallina.descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.gallinas.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.reporte.Folio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.statusgallina.descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
